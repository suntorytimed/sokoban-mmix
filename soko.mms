			LOC		#100

GPutChar	IS		#29
KGet		IS		#38

KUP			IS		#126
KDOWN		IS		#128
KLEFT		IS		#125
KRIGHT		IS		#127

char		IS		'@'
speed		IS		1
gap			IS		$0

WIDTH		IS		10
HEIGHT		IS		10
OFFSET		IS		100

% Modellkoordinaten
xMod		GREG	0
yMod		GREG	0

%___________________________________________________________________________________________
% Main-Methode für unser Programm

Main		SET		xMod,1					% Startposition für Sokoban
			SET		yMod,1
			PUSHJ	gap,:ShowBoard			% Anzeige des Spielfeldes

Loop		PUSHJ	gap,:ShowSokoban		% Sprung zu ShowSokoban

			TRAP	0,KGet,0
			SET		gap+1,$255
			PUSHJ	gap,:DoCommand			% Sprung zu DoCommand

			JMP		Loop					% Rücksprung zu Beginn der Schleife
%___________________________________________________________________________________________
% ShowSokoban gibt den Character an der berechneten Position aus. Der Startwert
% ist für xMod und yMod 0 und wird durch die Methode MoveSokoban beeinflusst.
% Für die Ausgabe rechnet ShowSokoban die Modellkoordinaten * 16

			PREFIX	Show:

xValue 		IS		$0
xyPos		IS		$1
char		IS		$2
xPos		IS		$3						% absolute Koordinaten
yPos		IS		$4


:ShowSokoban SETMH	char,:char				% Ausgabe erster Stern

			SL		xPos,:xMod,4
			SL		yPos,:yMod,4

			ADD		xPos,xPos,:OFFSET
			ADD		yPos,yPos,:OFFSET

			SLU		xValue,xPos,16			% Verschieben der x-Koordinaten in den ML
			OR		xyPos,xValue,yPos		% Zusammenführung der Koordinate
			OR		$255,char,xyPos			% Zusammenführung der Ausgabe ins Register 255

			TRAP	0,:GPutChar,0

			POP		0,0

			PREFIX	:
%___________________________________________________________________________________________
% DoCommand erhält als Parameter das KGet aus der Main-Methode
% Es gibt dxMod und dyMod je nach Tasteneingabe an MoveSokoban als Parameter weiter

			PREFIX 	DoCom:

Key			IS		$0						% $0 mit KGet als Parameter belegt
comp		IS		$1						% Hilfsvariable für den Vergleich
wayBack 	IS		$2
up			IS		$3
down		IS		$4
left		IS		$5
right		IS		$6
gap			IS		$7
dxMod		IS		gap+1
dyMod		IS		gap+2

:DoCommand	GET		wayBack,:rJ				% Spezialregister rJ sichern

			SET		dxMod,0
			SET		dyMod,0

			SET		up,:KUP
			SET		down,:KDOWN
			SET		left,:KLEFT
			SET		right,:KRIGHT

			CMP		comp,Key,up				% Vergleich der Tasteneingabe im Register
			BZ		comp,DirUp

			CMP		comp,Key,down
			BZ		comp,DirDown

			CMP		comp,Key,left
			BZ		comp,DirLeft

			CMP		comp,Key,right
			BZ		comp,DirRight

			POP		0,0						% Back-Jump falls Tasteneingabe falsch

DirUp		SUB		dyMod,dyMod,:speed		% Berechnung von Delta x und Delta y
			PUSHJ	gap,:MoveSokoban
			JMP		Exit

DirDown		ADD		dyMod,dyMod,:speed
			PUSHJ	gap,:MoveSokoban
			JMP		Exit

DirLeft		SUB		dxMod,dxMod,:speed
			PUSHJ	gap,:MoveSokoban
			JMP		Exit

DirRight	ADD		dxMod,dxMod,:speed
			PUSHJ	gap,:MoveSokoban
			JMP		Exit

Exit		PUT		:rJ,wayBack				% Ausstiegspunkt für das Unterprogramm bei Erfolg
			POP		0,0

			PREFIX	:
%___________________________________________________________________________________________
% MoveSokoban erhält als Parameter dxMod und dyMod von DoCommand
% Berechnet die neuen Modellkoordinaten in einem lokalen Register und ruft CheckPos auf
% Nach erfolgreicher Überprüfung durch CheckPos werden die globalen Register xMod und yMod
% mit den neuen Werten besetzt

			PREFIX	Move:

dxMod		IS		$0						% Parameter von DoCommand
dyMod		IS		$1						% Parameter von DoCommand
wayBack		IS		$2
cmp			IS		$3
gap			IS		$4						% Hier landet das Ergebnis von CheckPos
xMod		IS		gap+1					% Parameter für CheckPos
yMod		IS		gap+2					% Parameter für CheckPos
gap2		IS		gap+3
xOld		IS		gap+4
yOld		IS		gap+5

:MoveSokoban GET	wayBack,:rJ

			ADD 	xMod,:xMod,dxMod		% Berechnung der neuen Modellkoordinaten
			ADD		yMod,:yMod,dyMod

			PUSHJ	gap,:CheckPos
			CMP		cmp,gap,0				% Ist der Return-Wert von CheckPos korrekt?
			BZ		cmp,:SetNew

			PUT		:rJ,wayBack
			POP		0,0						% POP zurück in den entsprechenden BZ-Zweig

:SetNew		SET		xOld,:xMod
			SET		yOld,:yMod

			PUSHJ	gap2,:ShowField
			
			SET		:xMod,xMod				% Setzen der lokalen Modellkoordinaten in die
			SET		:yMod,yMod				% globalen Register, wenn CheckPos erfolgreich

			PUT		:rJ,wayBack
			POP		0,0						% POP zurück in den entsprechenden BZ-Zweig

			PREFIX	:
%___________________________________________________________________________________________
% CheckPos erhält als Parameter xMod und yMod aus MoveSokoban
% Überprüft, ob die neuen xMod und yMod-Koordinaten erlaubt sind
% Die Rückgabewerte sind gap: return, gap+1: xMod und gap+2: yMod

			PREFIX	Check:

xMod		IS		$0
yMod		IS		$1
return		IS		$2
cmp			IS		$3
HEIGHT		IS		$4
WIDTH		IS		$5
ELEMENT		IS		$6
OFFSET		IS		$7
base		IS		$8

:CheckPos	SET		HEIGHT,:HEIGHT
			SET		WIDTH,:WIDTH
			SET		ELEMENT,0
			SET		OFFSET,0
			
			LDA		base,:GameBoard
			MUL		OFFSET,yMod,WIDTH
			ADD		OFFSET,OFFSET,xMod
			LDB		ELEMENT,base,OFFSET
			

			CMP		cmp,HEIGHT,yMod			% Wenn +1 korrekt
			BNP		cmp,:Fail				% Wenn cmp negativ dann failure
			CMP		cmp,WIDTH,xMod
			BNP		cmp,:Fail
			CMP		cmp,yMod,0				% Wenn im Bereich 0 und +1 korrekt
			BN		cmp,:Fail				% Wenn cmp negativ dann failure
			CMP		cmp,xMod,0
			BN		cmp,:Fail
			CMP		cmp,ELEMENT,:S			% Ist Element ein Stein?
			BZ		cmp,:Fail				% Wenn cmp 0 dann failure
			CMP		cmp,ELEMENT,:W			% Ist Element eine Wand?
			BZ		cmp,:Fail				% Wenn cmp 0 dann failure
			

			SET		return,0
			POP		3,0						% Erfolgreich! return in $0
											% 3 Return-Werte zum safen von x- und y-Mod

:Fail		SET		return,1
			POP		3,0						% Fehlschlag! return in $0
											% 3 Return-Werte zum safen von x- und y-Mod

			PREFIX	:
%___________________________________________________________________________________________
% ShowBoard erledigt den Schleifenaufruf für jede x und y Koordinate des Spielfeldes um
% darzustellen.

			PREFIX	ShowBoard:

cmp			IS		$0
wayBack		IS		$1
gap			IS		$2
xCounter	IS		gap+1
yCounter	IS		gap+2

:ShowBoard	GET		wayBack,:rJ
			SET		xCounter,0
			SET		yCounter,0
			
			PUSHJ	gap,:ShowField		% setzen des Elements bei 0,0
			
Counter		ADD 	xCounter,xCounter,1
			CMP		cmp,xCounter,:WIDTH
			BZ		cmp,NextRow

			PUSHJ	gap,:ShowField

			JMP		Counter

NextRow		SET		xCounter,0
			ADD 	yCounter,yCounter,1
			CMP		cmp,yCounter,:HEIGHT
			BZ		cmp,End
			
			PUSHJ	gap,:ShowField
			
			JMP		Counter

End			PUT		:rJ,wayBack
			POP		0,0
			
			PREFIX	:
			
%___________________________________________________________________________________________
% ShowField erledigt den Schleifenaufruf für jede x und y Koordinate des Spielfeldes um
% darzustellen.		

			PREFIX	ShowField:
			
xCounter	IS		$0
yCounter	IS		$1
base		IS		$2
OFFSET		IS		$3
ELEMENT		IS		$4
xValue 		IS		$5
xyPos		IS		$6
char		IS		$7
xPos		IS		$8						% absolute Koordinaten
yPos		IS		$9
cmp			IS		$10

:ShowField	LDA		base,:GameBoard
			MUL		OFFSET,yCounter,:WIDTH
			ADD		OFFSET,OFFSET,xCounter
			LDB		ELEMENT,base,OFFSET		% Enthält den Zahlencode für W, S, F oder T
			
			CMP		cmp,ELEMENT,:W			% Handelt es sich um eine Wand?
			BZ		cmp,Wall				% Springt wenn ja
			
			CMP		cmp,ELEMENT,:S			% Handelt es sich um einen Stein?
			BZ		cmp,Stone				% Springt wenn ja
			
			CMP		cmp,ELEMENT,:F			% Handelt es sich um ein freies Feld?
			BZ		cmp,Free				% Springt wenn ja
			
			CMP		cmp,ELEMENT,:T			% Handelt es sich um das Ziel?
			BZ		cmp,Target				% Springt wenn ja
			
Wall		SETMH	char,'#'
			JMP		Output
Stone		SETMH	char,'o'
			JMP		Output
Free		SETMH	char,'.'
			JMP		Output
Target		SETMH	char,'x'
			JMP		Output

Output		SL		xPos,xCounter,4
			SL		yPos,yCounter,4

			ADD		xPos,xPos,:OFFSET
			ADD		yPos,yPos,:OFFSET

			SLU		xValue,xPos,16			% Verschieben der x-Koordinaten in den ML
			OR		xyPos,xValue,yPos		% Zusammenführung der Koordinate
			OR		$255,char,xyPos			% Zusammenführung der Ausgabe ins Register 255

			TRAP	0,:GPutChar,0
			
			POP		0,0
			
			PREFIX	: